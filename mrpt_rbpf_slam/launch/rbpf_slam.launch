<launch>
    <!--PARAMETERS-->
    <param name="ini_filename" value="$(find mrpt_rbpf_slam)/tutorial/grid_slam_demo.ini"/>
    <param name="odom_frame_id" value="r1/odom"/>
    <param name="global_frame_id" value="/map"/>
    <param name="base_frame_id" value="r1/base_link"/>
    <param name="sensor_source" value="r1/front_laser/scan"/> <!--for multiple sensors, place comma eg "r1/front_laser/scan,/beacon" -->

    <!--PLAY BAG FILE-->
  <include file="$(find mrpt_rawlog)/launch/demo_rosbag.launch" />

  
    <!--SLAM RUN--> 
  <node pkg="rviz" type="rviz" name="rviz_nav" output="screen" args="-d $(find mrpt_rbpf_slam)/rviz/rviz_conf.rviz"></node>
  <node pkg="mrpt_rbpf_slam" type="mrpt_rbpf_slam" name="mrpt_rbpf_slam" output="screen"></node>
  
</launch>

